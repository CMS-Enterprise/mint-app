      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "loki"
          },
          "expr": "sum by (service) (count_over_time({service=~\"$service\", container=~\"$container\"}[$__interval]))",
          "refId": "A"
        }
      ],
      "title": "Log Rate by Service",
      "type": "timeseries"
    },
