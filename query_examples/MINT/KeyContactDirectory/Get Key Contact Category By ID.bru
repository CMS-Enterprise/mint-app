meta {
  name: Get Key Contact Category By ID
  type: graphql
  seq: 9
}

post {
  url: {{url}}
  body: graphql
  auth: apikey
}

auth:apikey {
  key: Authorization
  value: Local {"EUAID":"{{EUAID}}","jobCodes":{{jobCodes}},"favorLocalAuth":true}
  placement: header
}

body:graphql {
  query getKeyContactCategoryById($id: UUID!) {
    keyContactCategoryById(id: $id) {
      id
      name
    }
  }
}

body:graphql:vars {
  {
    "id": "{{keyContactCategoryID}}"
  }
}
