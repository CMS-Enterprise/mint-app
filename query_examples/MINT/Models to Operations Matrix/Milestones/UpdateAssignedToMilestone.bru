meta {
  name: UpdateAssignedToMilestone
  type: graphql
  seq: 9
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  mutation updateAssignedToMTOMilestone {
    updateAssignedToMTOMilestone(
      id: "{{mtoMilestoneID}}"
      assignedTo: "{{currentUserID}}"
    ) {
      id
      assignedTo
      assignedToUserAccount {
        id
        username
      }
    }
  }
}

settings {
  encodeUrl: true
}
