meta {
  name: Apply Template
  type: graphql
  seq: 3
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  mutation ApplyMtoTemplate {
    createTemplateToMTO(
      modelPlanID: "12345678-1234-1234-1234-123456789012"
      templateID: "00000000-0000-0000-0000-000000000000"
    ) {
      id
      modelPlanID
      templateID
      categoriesAdded
      milestonesAdded
      solutionsAdded
      warnings
    }
  }
}

settings {
  encodeUrl: true
}
