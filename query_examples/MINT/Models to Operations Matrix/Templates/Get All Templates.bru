meta {
  name: Get All Templates
  type: graphql
  seq: 1
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  query GetAllMtoTemplates {
    mtoTemplates(keys: INCLUDE_ALL) {
      id
      key
      name
      description
      categoryCount
      milestoneCount
      solutionCount
      createdBy
      createdDts
      modifiedBy
      modifiedDts
    }
  }
}

settings {
  encodeUrl: true
}
