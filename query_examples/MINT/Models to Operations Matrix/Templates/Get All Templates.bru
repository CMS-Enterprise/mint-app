meta {
  name: Get All Templates
  type: graphql
  seq: 1
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  query GetAllMtoTemplates {
    mtoTemplates {
      id
      key
      name
      description
      categoryCount
      milestoneCount
      solutionCount
      solutions{
        id
      }
    }
  }
}

settings {
  encodeUrl: true
}
