meta {
  name: GetMTO Contractor
  type: graphql
  seq: 1
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  query GetMTOCommonSolutionContractor {
    mtoCommonSolutionContractor(id: "{{contractorId}}") {
      id
      key
      contractorTitle
      contractorName
    }
  }
}

