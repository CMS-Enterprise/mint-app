meta {
  name: Delete MTO ContactInfo
  type: graphql
  seq: 6
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  mutation DeleteMTOCommonSolutionContact {
    deleteMTOCommonSolutionContact(id: "{{contactId}}") {
      id
      key
      mailboxTitle
      mailboxAddress
      isTeam
      role
      isPrimary
      receiveEmails
      createdBy
    }
  }
}

vars:pre-request {
  contactId: 2a1f8bbe-7017-43d9-9f8e-ed7c6561c807
}
