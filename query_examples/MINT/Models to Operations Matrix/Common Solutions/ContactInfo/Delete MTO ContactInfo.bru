meta {
  name: Delete MTO ContactInfo
  type: graphql
  seq: 6
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  mutation DeleteMTOCommonSolutionContact {
    deleteMTOCommonSolutionContact(id: "{{contactId}}") {
      id
      key
      mailboxTitle
      mailboxAddress
      isTeam
      role
      isPrimary
      receiveEmails
      createdBy
    }
  }
}

vars:pre-request {
  contactId: cba3a527-3b0a-4104-89bc-569a6e33aed6
}
