meta {
  name: Delete MTO ContactInfo
  type: graphql
  seq: 6
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  mutation DeleteMTOCommonSolutionContact {
    deleteMTOCommonSolutionContact(id: "{{contactId}}") {
      id
      key
      mailboxTitle
      mailboxAddress
      isTeam
      role
      isPrimary
      receiveEmails
      createdBy
    }
  }
}

vars:pre-request {
  contactId: 1f2c8607-d06f-42bd-ba31-0e70e069f3d9
}
