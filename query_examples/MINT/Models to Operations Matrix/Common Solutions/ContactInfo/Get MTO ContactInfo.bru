meta {
  name: Get MTO ContactInfo
  type: graphql
  seq: 3
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  query GetMTOCommonSolutionContact {
    mtoCommonSolutionContact(id: "{{contactId}}") {
      id
      key
      mailboxTitle
      mailboxAddress
      isTeam
      role
      isPrimary
      receiveEmails
      createdBy
      userID
    }
  }
}

vars:pre-request {
  contactId: 2a1f8bbe-7017-43d9-9f8e-ed7c6561c807
}
