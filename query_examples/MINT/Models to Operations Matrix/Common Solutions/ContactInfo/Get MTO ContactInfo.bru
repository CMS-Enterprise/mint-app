meta {
  name: Get MTO ContactInfo
  type: graphql
  seq: 3
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  query GetMTOCommonSolutionContact {
    mtoCommonSolutionContact(id: "{{contactId}}") {
      id
      key
      mailboxTitle
      mailboxAddress
      isTeam
      role
      isPrimary
      receiveEmails
      createdBy
    }
  }
}

vars:pre-request {
  contactId: 2604ea52-f8b8-42f4-90aa-dff1cf9668c2
}
