meta {
  name: Get MTO ContactInfo
  type: graphql
  seq: 3
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  query GetMTOCommonSolutionContact {
    mtoCommonSolutionContact(id: "{{contactId}}") {
      id
      key
      mailboxTitle
      mailboxAddress
      isTeam
      role
      isPrimary
      receiveEmails
      createdBy
    }
  }
}

vars:pre-request {
  contactId: 71f20ff5-be2f-47be-aca1-117df40ae839
}
