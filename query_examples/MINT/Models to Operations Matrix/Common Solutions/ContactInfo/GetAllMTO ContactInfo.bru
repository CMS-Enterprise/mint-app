meta {
  name: GetAllMTO ContactInfo
  type: graphql
  seq: 4
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  query GetAllContractors {
    mtoCommonSolutions {
      contactInformation {
        primaryContact {
          mailboxTitle
          mailboxAddress
          isTeam
          role
          isPrimary
          userAccount {
            id
            username
            givenName
            familyName
          }
        }
        pointsOfContact {
          mailboxTitle
          mailboxAddress
          isTeam
          role
          isPrimary
          userAccount {
            id
            username
            givenName
            familyName
          }
        }
      }
    }
  }
  
}
