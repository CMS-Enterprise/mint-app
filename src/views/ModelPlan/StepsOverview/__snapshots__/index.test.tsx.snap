// Vitest Snapshot v1, https://vitest.dev/guide/snapshot.html

exports[`The Model Plan Steps Overview static page > matches the snapshot 1`] = `
<main
  className="mint-main-content"
  id="main-content"
  role="main"
  tabIndex={-1}
>
  <div
    className="grid-container"
  >
    <div
      className="tablet:grid-col-12"
    >
      <nav
        aria-label="Breadcrumbs"
        className="usa-breadcrumb usa-breadcrumb--wrap"
      >
        <ol
          className="usa-breadcrumb__list"
        >
          <li
            className="usa-breadcrumb__list-item"
          >
            <a
              className="usa-link usa-breadcrumb__link"
              href="/"
              onClick={[Function]}
            >
              <span>
                Home
              </span>
            </a>
          </li>
          <li
            aria-current="page"
            className="usa-breadcrumb__list-item usa-current"
          >
            <span>
              Start a new model plan
            </span>
          </li>
        </ol>
      </nav>
      <h1
        aria-live="polite"
        className="mint-h1"
        tabIndex={-1}
      >
        Start a new model plan
      </h1>
      <div
        className="usa-summary-box bg-base-lightest border-0 radius-0 padding-y-2 padding-x-2"
        data-testid="summary-box"
      >
        <div
          className="usa-summary-box__body"
        >
          <p
            className="margin-top-0 margin-bottom-1"
          >
            Use this service to:
          </p>
          <ul
            className="padding-left-3 margin-y-0"
          >
            <li>
              start a new Model Plan to analyze the operational and implementation needs of the model
            </li>
            <li>
              upload and access documents related to your model request, like concept documents or policy papers
            </li>
            <li>
              track the operational solutions the model will use and track its status through implementation
            </li>
          </ul>
          <p
            className="margin-y-1"
          >
            If you have any questions, you can reach the MINT Team at: 
            <a
              className="usa-link"
              href="mailto:MINTTeam@cms.hhs.gov"
            >
              MINTTeam@cms.hhs.gov
            </a>
            .
          </p>
        </div>
      </div>
      <h2
        aria-live="polite"
        className="mint-h1 margin-top-7 margin-bottom-1"
        tabIndex={-1}
      >
        Steps involved in the process
      </h2>
      <p
        className="font-body-lg margin-y-0"
      >
        Below is an overview of the process involved in creating your draft model plan.
      </p>
      <div
        className="desktop:grid-col-6 margin-top-105"
        data-testid="grid"
      >
        <ol
          className="usa-process-list"
        >
          <li
            className="usa-process-list__item"
          >
            <h3
              className="usa-process-list__heading"
              data-testid="processListHeading"
            >
              Fill out the sections of the draft model plan form and document model requirements
            </h3>
            <p>
              Tell the MINT Team about your model and collaborate with the rest of your team to draft model requirements around participants, population, operations, payment options, and more.
            </p>
          </li>
          <li
            className="usa-process-list__item"
          >
            <h3
              className="usa-process-list__heading"
              data-testid="processListHeading"
            >
              Upload any existing model documentation
            </h3>
            <p>
              Add additional details to your Model Plan by uploading documents such as a concept paper, draft ICIP, or other related materials.
            </p>
          </li>
          <li
            className="usa-process-list__item padding-bottom-3"
          >
            <h3
              className="usa-process-list__heading"
              data-testid="processListHeading"
            >
              Iterate on your draft model plan
            </h3>
            <p>
              Work with the MINT Team and other administrative teams to iterate on model requirements.
            </p>
          </li>
          <li
            className="usa-process-list__item"
          >
            <h3
              className="usa-process-list__heading"
              data-testid="processListHeading"
            >
              Track your operational solutions
            </h3>
            <p>
              Work with your IT Lead or BSG to determine which solutions your model will use for the operational needs. Then, track the implementation status of those solutions to stay organized throughout the process.
            </p>
          </li>
          <li
            className="usa-process-list__item"
          >
            <h3
              className="usa-process-list__heading"
              data-testid="processListHeading"
            >
              Finalize your Model Plan and prepare for clearance
            </h3>
            <p>
              Review all sections of your Model Plan and confirm that your model is ready for internal clearance processes and matches your clearance documentation.
            </p>
          </li>
        </ol>
      </div>
      <a
        className="usa-button margin-bottom-10"
        data-testid="continue-link"
        href="/models/new-plan"
        onClick={[Function]}
      >
        Get started
      </a>
    </div>
  </div>
</main>
`;
